{
  "name": "nextjs-boilerplate",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules yarn.lock yarn-error.log tsconfig.tsbuildinfo .stylelintcache coverage .next .swc out storybook-static",
    "prepare": "husky install",
    "preinstall": "npx only-allow yarn",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:next": "next lint -d src --fix",
    "lint:style": "stylelint 'src/**/*.tsx' -f verbose --fix --cache",
    "prettier": "prettier src --write --cache",
    "check-types": "tsc --noEmit",
    "lint": "yarn prettier && yarn check-types && yarn lint:next && yarn lint:style",
    "test": "jest --passWithNoTests --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collect-coverage",
    "test:update-snapshot": "jest --update-snapshot",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "lint-staged": {
    "*.[jt]s?(x)": [
      "yarn lint",
      "yarn test"
    ]
  },
  "dependencies": {
    "@emotion/cache": "11.10.5",
    "@emotion/react": "11.10.6",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.6",
    "@mui/material": "5.11.14",
    "lodash": "4.17.21",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "17.4.0",
    "type-fest": "3.7.1"
  },
  "devDependencies": {
    "@emotion/jest": "11.10.5",
    "@faker-js/faker": "7.6.0",
    "@next/bundle-analyzer": "13.2.4",
    "@storybook/addon-a11y": "7.0.0-rc.8",
    "@storybook/addon-essentials": "7.0.0-rc.8",
    "@storybook/addon-interactions": "7.0.0-rc.8",
    "@storybook/addon-links": "7.0.0-rc.8",
    "@storybook/addon-mdx-gfm": "7.0.0-rc.8",
    "@storybook/nextjs": "7.0.0-rc.8",
    "@storybook/react": "7.0.0-rc.8",
    "@storybook/testing-library": "0.0.14-next.1",
    "@svgr/webpack": "7.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/lodash": "4.14.192",
    "@types/node": "18.15.10",
    "@types/react": "18.0.30",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "eslint": "8.36.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.11",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "13.2.0",
    "next-compose-plugins": "2.2.1",
    "postcss-styled-syntax": "0.4.0",
    "prettier": "2.8.7",
    "storybook": "7.0.0-rc.8",
    "stylelint": "15.3.0",
    "stylelint-config-recess-order": "4.0.0",
    "stylelint-config-recommended": "11.0.0",
    "typescript": "5.0.2"
  }
}
