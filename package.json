{
  "name": "nextjs-boilerplate",
  "private": true,
  "engines": {
    "node": "18.18.2"
  },
  "packageManager": "yarn@3.6.4",
  "scripts": {
    "clean": "rm -rf node_modules yarn.lock yarn-error.log tsconfig.tsbuildinfo .stylelintcache coverage .next .swc out storybook-static .yarn/cache .yarn/install-state.gz",
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:next": "next lint -d src --fix",
    "lint:style": "stylelint 'src/**/*.tsx' -f verbose --fix --cache",
    "prettier": "prettier src --write --cache",
    "check-types": "tsc --noEmit",
    "lint": "yarn prettier && yarn check-types && yarn lint:next && yarn lint:style",
    "test": "jest --passWithNoTests --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collect-coverage",
    "test:update-snapshot": "jest --update-snapshot",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "1.2.1",
    "body-scroll-lock-upgrade": "1.0.4",
    "lodash-es": "4.17.21",
    "next": "13.5.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-focus-lock": "2.9.6",
    "react-merge-refs": "2.1.1",
    "react-popper": "2.3.0",
    "react-transition-group": "4.4.5",
    "react-use": "17.4.0",
    "styled-components": "6.1.0",
    "type-fest": "4.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "8.2.0",
    "@next/bundle-analyzer": "13.5.5",
    "@popperjs/core": "2.11.8",
    "@storybook/addon-a11y": "7.4.6",
    "@storybook/addon-essentials": "7.4.6",
    "@storybook/addon-interactions": "7.4.6",
    "@storybook/addon-links": "7.4.6",
    "@storybook/nextjs": "7.4.6",
    "@storybook/react": "7.4.6",
    "@storybook/testing-library": "0.2.2",
    "@svgr/webpack": "8.1.0",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/jest": "29.5.5",
    "@types/lodash-es": "4.17.9",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "@types/react-transition-group": "4.4.7",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "eslint": "8.51.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-next": "13.5.5",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-storybook": "0.6.15",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-styled-components": "7.2.0",
    "lint-staged": "14.0.1",
    "next-compose-plugins": "2.2.1",
    "postcss-styled-syntax": "0.5.0",
    "prettier": "3.0.3",
    "storybook": "7.4.6",
    "stylelint": "15.10.3",
    "stylelint-config-recess-order": "4.3.0",
    "stylelint-config-recommended": "13.0.0",
    "typescript": "5.2.2"
  },
  "resolutions": {
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13"
  }
}
