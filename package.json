{
  "name": "nextjs-boilerplate",
  "private": true,
  "engines": {
    "node": "20.11.0"
  },
  "packageManager": "yarn@4.1.0",
  "scripts": {
    "clean": "rm -rf node_modules yarn.lock yarn-error.log tsconfig.tsbuildinfo coverage .next .swc out storybook-static .yarn/cache .yarn/install-state.gz",
    "prepare": "husky",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:next": "next lint -d src --fix",
    "prettier": "prettier src --write --cache",
    "check-types": "tsc --noEmit",
    "lint": "yarn prettier && yarn check-types && yarn lint:next",
    "test": "jest --passWithNoTests --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collect-coverage",
    "test:update-snapshot": "jest --update-snapshot",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@tanstack/react-query": "5.17.19",
    "@tanstack/react-query-devtools": "5.17.21",
    "@tanstack/react-query-next-experimental": "5.17.19",
    "@yamada-ui/react": "1.2.2",
    "axios": "1.6.7",
    "change-case": "5.4.2",
    "lodash-es": "4.17.21",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "17.5.0",
    "type-fest": "4.10.2"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.0",
    "@next/bundle-analyzer": "14.1.0",
    "@storybook/addon-a11y": "7.6.10",
    "@storybook/addon-essentials": "7.6.10",
    "@storybook/addon-interactions": "7.6.10",
    "@storybook/addon-links": "7.6.10",
    "@storybook/nextjs": "7.6.10",
    "@storybook/react": "7.6.10",
    "@storybook/testing-library": "0.2.2",
    "@svgr/webpack": "8.1.0",
    "@tanstack/eslint-plugin-query": "5.17.22",
    "@testing-library/jest-dom": "6.4.0",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.11",
    "@types/lodash-es": "4.17.12",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "eslint": "8.56.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.3",
    "eslint-plugin-storybook": "0.6.15",
    "husky": "9.0.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.2.0",
    "next-compose-plugins": "2.2.1",
    "postcss-preset-env": "9.3.0",
    "prettier": "3.2.4",
    "storybook": "7.6.10",
    "typescript": "5.3.3"
  },
  "resolutions": {
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18"
  }
}
