{
  "name": "nextjs-boilerplate",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules yarn.lock yarn-error.log tsconfig.tsbuildinfo .stylelintcache coverage .next .swc out storybook-static",
    "prepare": "husky install",
    "preinstall": "npx only-allow yarn",
    "dev": "next dev",
    "build": "next build && next export && next-export-optimize-images",
    "start": "next start",
    "lint:next": "next lint --dir src",
    "lint:style": "stylelint 'src/**/*.{ts,tsx}' -f verbose --fix --cache",
    "prettier": "prettier 'src/**/*.{ts,tsx}' --write --cache",
    "lint": "yarn tsc && yarn lint:next && yarn lint:style && yarn prettier",
    "test": "jest --passWithNoTests",
    "test:coverage": "yarn test --collect-coverage",
    "test:update-snapshot": "yarn test --update-snapshot",
    "analyze": "ANALYZE=true next build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.[jt]s?(x)": [
      "yarn lint",
      "yarn test"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.0",
    "framer-motion": "7.6.7",
    "lodash": "4.17.21",
    "next": "13.0.4",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-focus-lock": "2.9.2",
    "react-redux": "8.0.5",
    "scroll-lock": "2.1.5",
    "styled-breakpoints": "11.1.1",
    "styled-components": "6.0.0-beta.6",
    "type-fest": "3.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@faker-js/faker": "7.6.0",
    "@next/bundle-analyzer": "13.0.4",
    "@storybook/addon-a11y": "7.0.0-alpha.51",
    "@storybook/addon-actions": "7.0.0-alpha.51",
    "@storybook/addon-essentials": "7.0.0-alpha.51",
    "@storybook/addon-interactions": "7.0.0-alpha.51",
    "@storybook/addon-links": "7.0.0-alpha.51",
    "@storybook/builder-webpack5": "7.0.0-alpha.51",
    "@storybook/manager-webpack5": "7.0.0-alpha.10",
    "@storybook/react": "7.0.0-alpha.51",
    "@storybook/testing-library": "0.0.14-next.0",
    "@stylelint/postcss-css-in-js": "0.38.0",
    "@svgr/webpack": "6.5.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.3",
    "@types/lodash": "4.14.189",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/scroll-lock": "2.1.0",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "babel-loader": "9.1.0",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-storybook": "0.6.7",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jest-styled-components": "7.1.1",
    "lint-staged": "13.0.3",
    "next-compose-plugins": "2.2.1",
    "next-export-optimize-images": "1.9.0",
    "postcss-syntax": "0.36.2",
    "prettier": "2.7.1",
    "storybook-addon-next": "1.6.10",
    "storybook-dark-mode": "1.1.3-canary.71.4216.0",
    "stylelint": "14.15.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-no-unsupported-browser-features": "6.0.1",
    "stylelint-prettier": "2.0.0",
    "typescript": "4.9.3"
  }
}
