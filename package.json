{
  "name": "nextjs-boilerplate",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules yarn.lock yarn-error.log tsconfig.tsbuildinfo .next dist",
    "prepare": "husky install",
    "preinstall": "npx only-allow yarn",
    "dev": "next dev",
    "build": "next build && next export && next-export-optimize-images",
    "start": "next start",
    "lint:next": "next lint --dir src",
    "lint:markup": "markuplint **/*.tsx",
    "lint:style": "stylelint **/*.{ts,tsx} -f verbose --fix --cache",
    "lint": "yarn lint:next && yarn lint:markup && yarn lint:style",
    "test": "yarn jest",
    "test:coverage": "yarn jest --collect-coverage",
    "analyze": "ANALYZE=true next build"
  },
  "lint-staged": {
    "*.[jt]s?(x)": "yarn lint"
  },
  "dependencies": {
    "framer-motion": "7.3.5",
    "next": "12.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-focus-lock": "2.9.1",
    "scroll-lock": "2.1.5",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "2.4.2",
    "@markuplint/react-spec": "2.1.1",
    "@next/bundle-analyzer": "12.3.0",
    "@reduxjs/toolkit": "1.8.5",
    "@stylelint/postcss-css-in-js": "0.38.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.0.3",
    "@types/lodash": "4.14.185",
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/scroll-lock": "2.1.0",
    "@types/styled-components": "5.1.26",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.1",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "jest-styled-components": "7.1.1",
    "lint-staged": "13.0.3",
    "markuplint": "2.10.1",
    "next-compose-plugins": "2.2.1",
    "next-export-optimize-images": "1.7.0",
    "postcss-syntax": "0.36.2",
    "react-redux": "8.0.2",
    "stylelint": "14.12.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-no-unsupported-browser-features": "5.0.4",
    "stylelint-prettier": "2.0.0",
    "typescript": "4.8.3"
  }
}
