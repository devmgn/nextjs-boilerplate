{
  "name": "nextjs-boilerplate",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules yarn.lock yarn-error.log tsconfig.tsbuildinfo .stylelintcache coverage .next .swc out storybook-static",
    "prepare": "husky install",
    "preinstall": "npx only-allow yarn",
    "dev": "next dev",
    "build": "next build && next export && next-export-optimize-images",
    "start": "next start",
    "lint:next": "next lint --dir src",
    "lint:style": "stylelint 'src/**/*.{ts,tsx}' -f verbose --fix --cache",
    "prettier": "prettier 'src/**/*.{ts,tsx}' --write --cache",
    "lint": "yarn tsc && yarn lint:next && yarn lint:style && yarn prettier",
    "test": "jest --passWithNoTests",
    "test:coverage": "yarn test --collect-coverage",
    "test:update-snapshot": "yarn test --update-snapshot",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "lint-staged": {
    "*.[jt]s?(x)": [
      "yarn lint",
      "yarn test"
    ]
  },
  "dependencies": {
    "@emotion/cache": "11.10.5",
    "@emotion/react": "11.10.6",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.6",
    "@mui/material": "5.11.12",
    "@reduxjs/toolkit": "1.9.3",
    "framer-motion": "10.2.5",
    "lodash": "4.17.21",
    "next": "13.2.4",
    "next-redux-wrapper": "9.0.0-rc.2",
    "postcss-styled-syntax": "^0.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-focus-lock": "2.9.4",
    "react-redux": "8.0.5",
    "react-use": "17.4.0",
    "scroll-lock": "2.1.5",
    "type-fest": "3.6.1"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@emotion/jest": "11.10.5",
    "@faker-js/faker": "7.6.0",
    "@next/bundle-analyzer": "13.2.4",
    "@storybook/addon-a11y": "7.0.0-rc.3",
    "@storybook/addon-essentials": "7.0.0-rc.3",
    "@storybook/addon-interactions": "7.0.0-rc.3",
    "@storybook/addon-links": "7.0.0-rc.3",
    "@storybook/addon-mdx-gfm": "7.0.0-rc.3",
    "@storybook/nextjs": "7.0.0-rc.3",
    "@storybook/react": "7.0.0-rc.3",
    "@storybook/testing-library": "0.0.14-next.1",
    "@stylelint/postcss-css-in-js": "0.38.0",
    "@svgr/webpack": "6.5.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.4.1",
    "@types/lodash": "4.14.191",
    "@types/node": "18.15.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/redux-logger": "^3.0.9",
    "@types/scroll-lock": "2.1.0",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "babel-loader": "9.1.2",
    "eslint": "8.36.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.11",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "13.2.0",
    "next-compose-plugins": "2.2.1",
    "prettier": "2.8.4",
    "storybook": "7.0.0-rc.3",
    "stylelint": "15.2.0",
    "stylelint-config-recess-order": "4.0.0",
    "stylelint-config-recommended": "10.0.1",
    "stylelint-no-unsupported-browser-features": "6.1.0",
    "stylelint-prettier": "3.0.0",
    "typescript": "4.9.5"
  }
}
