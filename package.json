{
  "name": "nextjs-boilerplate",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules yarn.lock yarn-error.log tsconfig.tsbuildinfo .next dist",
    "prepare": "husky install",
    "preinstall": "npx only-allow yarn",
    "dev": "next dev",
    "build": "next build && next export -o dist",
    "start": "next start",
    "lint:next": "next lint --dir src",
    "lint:markup": "markuplint **/*.tsx",
    "lint": "yarn lint:next && yarn lint:markup",
    "test": "yarn jest",
    "test:coverage": "yarn jest --collect-coverage",
    "analyze": "ANALYZE=true next build"
  },
  "lint-staged": {
    "*.[jt]s?(x)": "yarn lint"
  },
  "dependencies": {
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "styled-breakpoints": "11.1.0",
    "styled-components": "5.3.5",
    "swr": "1.3.0"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "2.4.1",
    "@markuplint/react-spec": "2.1.0",
    "@next/bundle-analyzer": "12.1.6",
    "@reduxjs/toolkit": "1.8.1",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/react-hooks": "8.0.0",
    "@testing-library/user-event": "14.2.0",
    "@types/jest": "27.5.1",
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@types/styled-components": "5.1.25",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "imagemin-mozjpeg": "10.0.0",
    "imagemin-pngquant": "9.0.2",
    "imagemin-svgo": "10.0.1",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "jest-styled-components": "7.0.8",
    "lint-staged": "12.4.1",
    "markuplint": "2.8.0",
    "next-compose-plugins": "2.2.1",
    "next-optimized-images": "3.0.0-canary.10",
    "react-redux": "8.0.1",
    "stylelint": "14.8.3",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended": "7.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "4.6.4"
  }
}
