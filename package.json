{
  "name": "nextjs-boilerplate",
  "private": true,
  "engines": {
    "node": "22.7.0"
  },
  "packageManager": "yarn@4.4.0",
  "scripts": {
    "postinstall": "node .husky/install.mjs",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn biome check --write --unsafe .",
    "check-types": "tsc --noEmit",
    "test": "jest --passWithNoTests --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collect-coverage",
    "test:update-snapshot": "jest --update-snapshot",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "bash -c \"yarn check-types\"",
      "yarn test"
    ]
  },
  "dependencies": {
    "@alice-health/ky-hooks-change-case": "0.0.3",
    "@hookform/resolvers": "3.9.0",
    "@lukemorales/query-key-factory": "1.3.4",
