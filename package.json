{
  "name": "nextjs-boilerplate",
  "private": true,
  "engines": {
    "node": "18.17.1"
  },
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:next": "next lint -d src --fix",
    "lint:style": "stylelint 'src/**/*.tsx' -f verbose --fix --cache",
    "prettier": "prettier src --write --cache",
    "check-types": "tsc --noEmit",
    "lint": "yarn prettier && yarn check-types && yarn lint:next && yarn lint:style",
    "test": "jest --passWithNoTests --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collect-coverage",
    "test:update-snapshot": "jest --update-snapshot",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "1.2.1",
    "@react-aria/utils": "3.18.0",
    "body-scroll-lock-upgrade": "1.0.4",
    "lodash-es": "4.17.21",
    "next": "13.4.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-focus-lock": "2.9.5",
    "react-merge-refs": "2.0.2",
    "react-popper": "2.3.0",
    "react-transition-group": "4.4.5",
    "react-use": "17.4.0",
    "styled-components": "6.0.7",
    "type-fest": "4.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@next/bundle-analyzer": "13.4.13",
    "@popperjs/core": "2.11.8",
    "@storybook/addon-a11y": "7.2.2",
    "@storybook/addon-essentials": "7.2.2",
    "@storybook/addon-interactions": "7.2.2",
    "@storybook/addon-links": "7.2.2",
    "@storybook/nextjs": "7.2.2",
    "@storybook/react": "7.2.2",
    "@storybook/testing-library": "0.2.0",
    "@svgr/webpack": "8.0.1",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/lodash-es": "4.17.8",
    "@types/react": "18.2.19",
    "@types/react-dom": "18.2.7",
    "@types/react-transition-group": "4.4.6",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "eslint": "8.46.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-next": "13.4.13",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-storybook": "0.6.13",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "jest-environment-jsdom": "29.6.2",
    "jest-styled-components": "7.1.1",
    "lint-staged": "13.2.3",
    "next-compose-plugins": "2.2.1",
    "postcss-styled-syntax": "0.4.0",
    "prettier": "3.0.1",
    "storybook": "7.2.2",
    "stylelint": "15.10.2",
    "stylelint-config-recess-order": "4.3.0",
    "stylelint-config-recommended": "13.0.0",
