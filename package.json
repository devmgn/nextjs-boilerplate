{
  "name": "nextjs-boilerplate",
  "private": true,
  "engines": {
    "node": "22.8.0"
  },
  "packageManager": "yarn@4.5.0",
  "scripts": {
    "postinstall": "node .husky/install.mjs",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix && yarn biome check --write --unsafe .",
    "check-types": "tsc --noEmit",
    "test": "jest --passWithNoTests --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collect-coverage",
    "test:update-snapshot": "jest --update-snapshot",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "bash -c \"yarn check-types\"",
      "yarn test"
    ]
  },
  "dependencies": {
    "@alice-health/ky-hooks-change-case": "0.0.3",
    "@hookform/resolvers": "3.9.0",
    "@lukemorales/query-key-factory": "1.3.4",
    "@radix-ui/react-dialog": "1.1.1",
    "@radix-ui/react-label": "2.1.0",
    "@radix-ui/react-slot": "1.1.0",
    "@tanstack/react-query": "5.56.2",
    "@tanstack/react-query-devtools": "5.56.2",
    "@tanstack/react-query-next-experimental": "5.56.2",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "ky": "1.7.2",
    "lodash-es": "4.17.21",
    "lucide-react": "0.441.0",
    "next": "15.0.0-rc.0",
    "react": "19.0.0-rc-fb9a90fa48-20240614",
    "react-dom": "19.0.0-rc-fb9a90fa48-20240614",
    "react-hook-form": "7.53.0",
    "react-use": "17.5.1",
    "tailwind-merge": "2.5.2",
    "tailwind-variants": "0.2.1",
    "type-fest": "4.26.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.1",
    "@faker-js/faker": "9.0.1",
    "@next/bundle-analyzer": "14.2.11",
    "@storybook/addon-a11y": "8.3.1",
    "@storybook/addon-essentials": "8.3.1",
    "@storybook/addon-interactions": "8.3.1",
    "@storybook/addon-links": "8.3.1",
    "@storybook/blocks": "8.3.1",
    "@storybook/nextjs": "8.3.1",
    "@storybook/react": "8.3.1",
    "@storybook/test": "8.3.1",
    "@svgr/webpack": "8.1.0",
    "@tanstack/eslint-plugin-query": "5.56.1",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.13",
    "@types/lodash-es": "4.17.12",
    "@types/node": "22.5.5",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "autoprefixer": "10.4.20",
    "babel-plugin-react-compiler": "0.0.0",
    "chromatic": "11.10.2",
    "eslint": "8.57.1",
    "eslint-config-next": "15.0.0-rc.0",
    "eslint-plugin-storybook": "0.8.0",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.2.10",
    "msw": "2.4.8",
    "msw-storybook-addon": "2.0.3",
    "next-compose-plugins": "2.2.1",
    "postcss": "8.4.47",
    "storybook": "8.3.1",
    "tailwindcss": "3.4.12",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.6.2"
  },
  "resolutions": {
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  },
  "msw": {
    "workerDirectory": ["public/"]
  }
}
