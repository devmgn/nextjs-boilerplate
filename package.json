{
  "name": "nextjs-boilerplate",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules yarn.lock yarn-error.log tsconfig.tsbuildinfo .stylelintcache coverage .next .swc out storybook-static",
    "prepare": "husky install",
    "preinstall": "npx only-allow yarn",
    "dev": "next dev",
    "build": "next build && next export && next-export-optimize-images",
    "start": "next start",
    "lint:next": "next lint --dir src",
    "lint:style": "stylelint 'src/**/*.{ts,tsx}' -f verbose --fix --cache",
    "prettier": "prettier 'src/**/*.{ts,tsx}' --write --cache",
    "lint": "yarn tsc && yarn lint:next && yarn lint:style && yarn prettier",
    "test": "jest --passWithNoTests",
    "test:coverage": "yarn test --collect-coverage",
    "test:update-snapshot": "yarn test --update-snapshot",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "*.[jt]s?(x)": [
      "yarn lint",
      "yarn test"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@reduxjs/toolkit": "1.9.1",
    "framer-motion": "8.0.2",
    "lodash": "4.17.21",
    "next": "13.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-focus-lock": "2.9.2",
    "react-redux": "8.0.5",
    "scroll-lock": "2.1.5",
    "type-fest": "3.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.7",
    "@emotion/jest": "11.10.5",
    "@faker-js/faker": "7.6.0",
    "@next/bundle-analyzer": "13.1.0",
    "@storybook/addon-a11y": "7.0.0-beta.12",
    "@storybook/addon-essentials": "7.0.0-beta.12",
    "@storybook/addon-interactions": "7.0.0-beta.12",
    "@storybook/addon-links": "7.0.0-beta.12",
    "@storybook/nextjs": "7.0.0-beta.12",
    "@storybook/react": "7.0.0-beta.12",
    "@storybook/testing-library": "0.0.14-next.1",
    "@stylelint/postcss-css-in-js": "0.38.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.4",
    "@types/lodash": "4.14.191",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/scroll-lock": "2.1.0",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "babel-loader": "9.1.0",
    "eslint": "8.30.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "13.1.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.7",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.8",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "lint-staged": "13.1.0",
    "next-compose-plugins": "2.2.1",
    "next-export-optimize-images": "2.0.0",
    "postcss-syntax": "0.36.2",
    "prettier": "2.8.1",
    "storybook": "7.0.0-beta.12",
    "stylelint": "14.16.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-recess-order": "3.1.0",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-no-unsupported-browser-features": "6.0.1",
    "stylelint-prettier": "2.0.0",
    "typescript": "4.9.4"
  }
}
