{
  "name": "nextjs-boilerplate",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "pnpm lint:oxlint && pnpm lint:fmt:check",
    "lint:oxlint": "oxlint --fix",
    "lint:fmt": "oxfmt --write .",
    "lint:fmt:check": "oxfmt --check .",
    "lint:knip": "knip --cache && knip --strict --cache",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:unit": "vitest run --project=unit",
    "test:update": "vitest run --update",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "analyze": "next experimental-analyze",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "pnpm dlx chromatic",
    "generate-api": "pnpm dlx @openapitools/openapi-generator-cli generate -i openapi.yml -g typescript-fetch -o ./src/api/openapi -c openapiconfig.json --openapi-normalizer SET_TAGS_FOR_ALL_OPERATIONS=default",
    "generate-api:clean": "rm -rf src/api/openapi && pnpm generate-api"
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "@lukemorales/query-key-factory": "1.3.4",
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/react-presence": "1.1.5",
    "@radix-ui/react-slot": "1.2.4",
    "@tanstack/react-query": "5.90.20",
    "@tanstack/react-query-devtools": "5.91.3",
    "es-toolkit": "1.44.0",
    "hono": "4.11.8",
    "next": "16.1.6",
    "nuqs": "2.8.8",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-error-boundary": "6.1.0",
    "react-hook-form": "7.71.1",
    "tailwind-merge": "3.4.0",
    "tailwind-variants": "3.2.2",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@faker-js/faker": "10.2.0",
    "@next/env": "16.1.6",
    "@storybook/addon-a11y": "10.2.7",
    "@storybook/addon-docs": "10.2.7",
    "@storybook/addon-vitest": "10.2.7",
    "@storybook/nextjs-vite": "10.2.7",
    "@tailwindcss/postcss": "4.1.18",
    "@tanstack/eslint-plugin-query": "5.91.4",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "24.10.11",
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.3",
    "@vitest/browser": "4.0.18",
    "@vitest/browser-playwright": "4.0.18",
    "@vitest/coverage-v8": "4.0.18",
    "axe-core": "4.11.1",
    "babel-plugin-react-compiler": "1.0.0",
    "chromatic": "15.0.0",
    "eslint": "9.39.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-storybook": "10.2.7",
    "jest-extended": "7.0.0",
    "jsdom": "28.0.0",
    "knip": "5.83.0",
    "msw": "2.12.8",
    "msw-storybook-addon": "2.0.6",
    "oxfmt": "0.28.0",
    "oxlint": "1.43.0",
    "playwright": "1.58.1",
    "storybook": "10.2.7",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "engines": {
    "node": "24.13.0"
  },
  "packageManager": "pnpm@10.28.2",
  "msw": {
    "workerDirectory": "./public"
  }
}
