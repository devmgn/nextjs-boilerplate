{
  "name": "nextjs-boilerplate",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules yarn.lock yarn-error.log tsconfig.tsbuildinfo .stylelintcache coverage .next .swc out storybook-static",
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:next": "next lint -d src --fix",
    "lint:style": "stylelint 'src/**/*.tsx' -f verbose --fix --cache",
    "prettier": "prettier src --write --cache",
    "check-types": "tsc --noEmit",
    "lint": "yarn prettier && yarn check-types && yarn lint:next && yarn lint:style",
    "test": "jest --passWithNoTests --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collect-coverage",
    "test:update-snapshot": "jest --update-snapshot",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "1.2.1",
    "lodash-es": "4.17.21",
    "next": "13.4.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "17.4.0",
    "styled-components": "6.0.0-rc.6",
    "type-fest": "3.12.0"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@next/bundle-analyzer": "13.4.7",
    "@storybook/addon-a11y": "7.0.23",
    "@storybook/addon-essentials": "7.0.23",
    "@storybook/addon-interactions": "7.0.23",
    "@storybook/addon-links": "7.0.23",
    "@storybook/nextjs": "7.0.23",
    "@storybook/react": "7.0.23",
    "@storybook/testing-library": "0.2.0",
    "@svgr/webpack": "8.0.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/lodash-es": "4.17.7",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "eslint": "8.43.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-storybook": "0.6.12",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-styled-components": "7.1.1",
    "lint-staged": "13.2.2",
    "next-compose-plugins": "2.2.1",
    "postcss-styled-syntax": "0.4.0",
    "prettier": "2.8.8",
    "storybook": "7.0.23",
    "stylelint": "15.9.0",
    "stylelint-config-recess-order": "4.2.0",
    "stylelint-config-recommended": "12.0.0",
    "typescript": "5.1.3"
  },
  "resolutions": {
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6"
  }
}
