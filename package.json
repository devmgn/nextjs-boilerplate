{
  "name": "nextjs-boilerplate",
  "private": true,
  "engines": {
    "node": "20.9.0"
  },
  "packageManager": "yarn@4.0.1",
  "scripts": {
    "clean": "rm -rf node_modules yarn.lock yarn-error.log tsconfig.tsbuildinfo coverage .next .swc out storybook-static .yarn/cache .yarn/install-state.gz",
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:next": "next lint -d src --fix",
    "prettier": "prettier src --write --cache",
    "check-types": "tsc --noEmit",
    "lint": "yarn prettier && yarn check-types && yarn lint:next",
    "test": "jest --passWithNoTests --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collect-coverage",
    "test:update-snapshot": "jest --update-snapshot",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@radix-ui/themes": "2.0.0",
    "@vanilla-extract/css": "1.13.0",
    "clsx": "2.0.0",
    "lodash-es": "4.17.21",
    "next": "14.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "17.4.0",
    "type-fest": "4.7.1"
  },
  "devDependencies": {
    "@faker-js/faker": "8.2.0",
    "@next/bundle-analyzer": "14.0.2",
    "@popperjs/core": "2.11.8",
    "@storybook/addon-a11y": "7.5.3",
    "@storybook/addon-essentials": "7.5.3",
    "@storybook/addon-interactions": "7.5.3",
    "@storybook/addon-links": "7.5.3",
    "@storybook/nextjs": "7.5.3",
    "@storybook/react": "7.5.3",
    "@storybook/testing-library": "0.2.2",
    "@svgr/webpack": "8.1.0",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.1.0",
    "@testing-library/user-event": "14.5.1",
    "@types/jest": "29.5.8",
    "@types/lodash-es": "4.17.11",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "@vanilla-extract/next-plugin": "2.3.1",
    "@vanilla-extract/webpack-plugin": "2.3.1",
    "eslint": "8.53.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-next": "14.0.2",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-storybook": "0.6.15",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.0.2",
    "next-compose-plugins": "2.2.1",
    "postcss-preset-env": "9.3.0",
    "prettier": "3.0.3",
    "storybook": "7.5.3",
    "typescript": "5.2.2"
  },
  "resolutions": {
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15"
  }
}
