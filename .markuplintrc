{
  "extends": [
    "markuplint:recommended"
  ],
  "rules": {
    "require-accessible-name": false
  },
  "parser": {
    ".[jt]sx?$": "@markuplint/jsx-parser"
  },
  "specs": [
    "@markuplint/react-spec"
  ],
  "excludeFiles": [
    "src/jest.setup.tsx",
    "src/**/*.{test,stories}.{ts,tsx}"
  ]
}
